{
  "version": 2,
  "builds": [
    {
      "src": "dist/my-app/**/*",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*).(js|css|png|jpg|jpeg|gif|ico|json)", "dest": "/dist/my-app/$1.$2" },
    { "src": "/(.*)", "dest": "/dist/my-app/index.html" }
  ]
}